<%= form_with model: [:admin, @group], local: true do |f| %>
<div>
	<%= f.label :name %>
	<%= f.text_field :name %>
</div>
<div>
	<%= f.label :course_id %>
	<%= f.select(:course_id, Course.all.collect {|c| [c.name, c.id]}, :prompt => 'Select') %>
	<!-- <%= f.text_field :course_id %> -->
</div>
<div>
	<%= f.label :start %>
	<%= f.date_field :start %>
</div>
<div>
	<%= f.label :end %>
	<%= f.date_field :end %>
</div>
<div>
	<%= f.submit %>
</div>
<% end %>